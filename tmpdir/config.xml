<PhysiCell_settings version="1.6.1">
	<domain>
		<x_min>-500.0</x_min>
		<x_max>500.0</x_max>
		<y_min>-500.0</y_min>
		<y_max>500.0</y_max>
		<z_min>-10.0</z_min>
		<z_max>10.0</z_max>
		<dx>20.0</dx>
		<dy>20.0</dy>
		<dz>20.0</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">1440.0</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	
		<dt_diffusion units="min">0.01</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">6</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>.</folder>  

		<full_data>
			<interval units="min">20</interval>
			<enable>True</enable>
		</full_data>
		
		<SVG>
			<interval units="min">20</interval>
			<enable>True</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<microenvironment_setup>
	
		<variable ID="0" name="thanos" units="dimensionless">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">100000.0</diffusion_coefficient>
				<decay_rate units="1/min">1.0</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="dimensionless">0.0</initial_condition>
			<Dirichlet_boundary_condition enabled="false" units="dimensionless">1.0</Dirichlet_boundary_condition>
		</variable>

		<variable ID="1" name="avenger" units="dimensionless">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">100000.0</diffusion_coefficient>
				<decay_rate units="1/min">1.0</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="dimensionless">0.0</initial_condition>
			<Dirichlet_boundary_condition enabled="false" units="dimensionless">1.0</Dirichlet_boundary_condition>
		</variable>

 
		
		<options>
			<calculate_gradients>True</calculate_gradients>
			<track_internalized_substrates_in_each_agent>False</track_internalized_substrates_in_each_agent>
			 
			<initial_condition enabled="false" type="matlab">
				<filename>./config/initial.mat</filename>
			</initial_condition>
			 
			<dirichlet_nodes enabled="false" type="matlab">
				<filename>./config/dirichlet.mat</filename>
			</dirichlet_nodes>
		</options>
	</microenvironment_setup>
	
	
	<cell_setup>
		
		<defaults>
			<cycle ID="" name="live">
				<transition_rates>
					<transition_rate fixed_duration="false" from="0" to="0" units="1/min" />
				</transition_rates>
			</cycle>
			<death ID="" name="apoptosis">
				<rate />
				<parameters /> 
			</death> 
			<death ID="" name="apoptosis">
				<rate />
				<parameters /> 
			</death> 
		</defaults> 
	
		<cycle_defaults>
		
		</cycle_defaults>
	
		<cell_definitions>
			<cell_definition ID="0" name="civilian">
				<phenotype>
					<cycle>
						<transition_rates>
						</transition_rates> 
					</cycle>
					<death />
					<geometry /> 
					<volume />
					<motility />
					<secretion />
					<mechanics /> 
				</phenotype>
			</cell_definition>
			
		</cell_definitions>
	
		<options>
			<cycle_model>live</cycle_model>
			<apoptosis_duration units="min">516</apoptosis_duration>
		</options> 
	
		<custom_data>
			<damage units="dimensionless">0.0</damage>
		</custom_data> 
		
		<initial_condition>
		</initial_condition> 
	
	</cell_setup>
	
	<user_parameters>
		<random_seed type="int" units="dimensionless">0</random_seed> 
		 
		
		 
		<civilian_initial_count description="initial number of civilians" type="int" units="none">500</civilian_initial_count>
		<civilian_speed description="civilian migration speed" type="double" units="micron/min">0.1</civilian_speed>
		<civilian_birth_rate description="civilian birth rate" type="double" units="1/min">0.0007</civilian_birth_rate>
		<civilian_death_rate description="civilian death rate" type="double" units="1/min">7e-06</civilian_death_rate>
		<civilian_strength description="civilian battle strength" type="double" units="dimensionless">0.01</civilian_strength> 
		
		
		<thanos_speed description="Thanos migration speed" type="double" units="micron/min">1.5</thanos_speed> 
		<thanos_snap_time hidden="true" type="double">720</thanos_snap_time>
		<thanos_strength description="Thanos battle strength" type="double" units="dimensionless">1000.0</thanos_strength>
		<thanos_motility_bias description="Thanos migration bias (0 = Brownian)" type="double" units="none">0.85</thanos_motility_bias> 
	
		 
		<avenger_initial_count description="number of Avengers" type="int" units="none">25</avenger_initial_count>
		<avenger_speed description="Avengers migration speed" type="double" units="micron/min">4.0</avenger_speed>
		<avenger_strength description="Avengers battle stregth" type="double" units="dimensionless">10.0</avenger_strength> 
		<avenger_motility_bias description="Avengers migration bias (0 = Brownian)" type="double" units="none">0.5</avenger_motility_bias> 
		<avenger_motility_bias_with_stone description="Avengers migration bias when hiding Infinity Stone" type="double" units="none">0.2</avenger_motility_bias_with_stone> 
		
	</user_parameters>
	
</PhysiCell_settings>